#include <stdio.h>
#include <stdlib.h>

struct key
{
 char c;
 int a[5];
 char *ch;
};

int main()
{
    printf("size of int:%d\n",sizeof int);
    printf("size of char:%d",sizeof char);
    printf("size of array:%d",sizeof key.a);
	printf("size of key:%d\n",sizeof(struct key));
	
	system("pause");
	return 0;
}
